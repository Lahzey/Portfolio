package poopgame.server;

import poopgame.gamelogic.Champion;
import poopgame.gamelogic.Player;

public class JoinRequest {

	public long id;
	public String name;
	public Champion champ;

	public JoinRequest(long id, String name, Champion champ) {
		super();
		this.id = id;
		this.name = name;
		this.champ = champ;
	}

}
