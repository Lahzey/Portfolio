package com.creditsuisse.graphics;

import java.awt.image.BufferedImage;

import javax.swing.ImageIcon;

/**
 * Old class before I used Ikonli.
 * <br/>Now stores Icons I need but are not available in Ikonli.
 * <br/>This is especially useful for complex images.
 * @author A469627
 *
 */
public enum Icon {

	LED(""),
	END_DONT_USE("");
	
	private String base64;
	private BufferedImage image;
	
	private Icon(String base64){
		this.base64 = base64;
	}
	
	public BufferedImage toImage(){
		if(image == null){
			image = ImageUtil.fromBase64(base64);
		}
		return image;
	}
	
	public ImageIcon toIcon(){
		return new ImageIcon(toImage());
	}
}
