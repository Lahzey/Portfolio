package poopgame.server;

import poopgame.gamelogic.Champion;

public class StartSignal {

	public long playerId;
	public String name;
	public Champion champ;
	
	public StartSignal() {}

	public StartSignal(long playerId, String name, Champion champ) {
		this.playerId = playerId;
		this.name = name;	
		this.champ = champ;
	}

}
